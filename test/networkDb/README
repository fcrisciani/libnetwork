SERVER

cd test/networkdb
env GOOS=linux go build -o gossipdb -v ../../networkdb/gossipdb/launcher/main.go
env GOOS=linux go build -v testMain.go
docker build -t fcrisciani/networkdb-test-grpc .
(only for testkit case) docker push fcrisciani/networkdb-test-grpc

Run server: docker service create --name testdb --network net1 --replicas 3 fcrisciani/networkdb-test-grpc server

CLIENT

cd test/networkdb
Init cluster: docker run -it --network net1 fcrisciani/networkdb-test-grpc client initialize testdb 5000
Join cluster: docker run -it --network net1 fcrisciani/networkdb-test client join testdb 8000
Join network: docker run -it --network net1 fcrisciani/networkdb-test client join-network testdb 8000 test
Run test: docker run -it --network net1 fcrisciani/networkdb-test client write-delete-unique-keys testdb 8000 test tableBla 3 10
check table: curl "localhost:32768/gettable?nid=test&tname=table_name"
